  <div class="container">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <h2>Welcome <span class="member-name"></span></h2>

        <form class="lookup">
          <div class="form-group">
            <label for="exampleInputMovie">Search a Movie</label>
            <input type="text" class="form-control" id="movie-input" placeholder="Movie Name">
          </div>
          
          <button type="submit" class="btnDefault">Search</button>
        </form>
      </div>
    </div>

    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <ul id="movieInfo">
          <div class="col-md-6">
     
<h2>Burgers that haven't been devoured!</h2>

<ul>
  {{#each Movie}}
    {{#unless watched}}
      {{> Movie/movie-block watched=true}}
    {{/unless}}
  {{/each}}
</ul>
</div>

<div class="col-md-6">
<h2>Burgers that have been devoured!</h2>

<ul>
  {{#each Movie}}
    {{#if watched}}
      {{> Movie/movie-block watched=false}}
    {{/if}}
  {{/each}}
</ul>
</div>

        </ul>
      </div>
    </div>

  </div>
  <input type="hidden" id="custId" name="custId" value="">
<script src="/assets/js/members.js"></script>